    <script>
        let currentFilter = 'all';
        let isVisible = false;
        
        // Intersection Observer –¥–ª—è –∞–Ω–∏–º–∞—Ü–∏–∏ –ø–æ—è–≤–ª–µ–Ω–∏—è
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('visible');
                }
            });
        }, { threshold: 0.1 });
        
        // –ù–∞–±–ª—é–¥–µ–Ω–∏–µ –∑–∞ —ç–ª–µ–º–µ–Ω—Ç–∞–º–∏
        document.querySelectorAll('.decade, .event').forEach(el => {
            observer.observe(el);
        });
        
        // –§—É–Ω–∫—Ü–∏–∏ —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏–∏
        function showAll() {
            currentFilter = 'all';
            updateActiveButton();
            document.querySelectorAll('.event').forEach(event => {
                event.style.display = 'block';
            });
        }
        
        function filterByType(type) {
            currentFilter = type;
            updateActiveButton();
            document.querySelectorAll('.event').forEach(event => {
                const categories = event.dataset.category || '';
                if (categories.includes(type)) {
                    event.style.display = 'block';
                } else {
                    event.style.display = 'none';
                }
            });
        }
        
        function updateActiveButton() {
            document.querySelectorAll('.nav-btn').forEach(btn => {
                btn.classList.remove('active');
            });
            
            const activeBtn = document.querySelector(`.nav-btn[onclick*="${currentFilter}"]`) || 
                             document.querySelector('.nav-btn[onclick="showAll()"]');
            if (activeBtn) {
                activeBtn.classList.add('active');
            }
        }
        
        // –ò–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω–æ—Å—Ç—å —Å–æ–±—ã—Ç–∏–π
        document.querySelectorAll('.event-content').forEach(event => {
            event.addEventListener('click', function() {
                // –≠—Ñ—Ñ–µ–∫—Ç –≤—ã–¥–µ–ª–µ–Ω–∏—è –ø—Ä–∏ –∫–ª–∏–∫–µ
                this.style.transform = 'scale(1.05)';
                this.style.background = 'rgba(245, 222, 179, 1)';
                this.style.borderColor = '#8B4513';
                
                setTimeout(() => {
                    this.style.transform = 'scale(1)';
                    this.style.background = 'rgba(245, 222, 179, 0.9)';
                    this.style.borderColor = 'rgba(139, 69, 19, 0.2)';
                }, 300);
            });
        });
        
        // –ü–ª–∞–≤–Ω–∞—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∞ –∫ –¥–µ—Å—è—Ç–∏–ª–µ—Ç–∏—è–º
        function scrollToDecade(decade) {
            const element = document.querySelector(`[data-decade="${decade}"]`);
            if (element) {
                element.scrollIntoView({ behavior: 'smooth' });
            }
        }
        
        // –ê–Ω–∏–º–∞—Ü–∏—è –∑–∞–≥—Ä—É–∑–∫–∏
        window.addEventListener('load', () => {
            setTimeout(() => {
                document.querySelector('.header').style.transform = 'translateY(0)';
                document.querySelector('.header').style.opacity = '1';
            }, 100);
        });
        
        // –ü–æ–¥—Å—á–µ—Ç —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
        function updateStats() {
            const totalEvents = document.querySelectorAll('.event').length;
            const majorEvents = document.querySelectorAll('.event.major').length;
            const politicalEvents = document.querySelectorAll('.event[data-category*="politics"]').length;
            
            console.log(`–í—Å–µ–≥–æ —Å–æ–±—ã—Ç–∏–π: ${totalEvents}`);
            console.log(`–ö–ª—é—á–µ–≤—ã—Ö —Å–æ–±—ã—Ç–∏–π: ${majorEvents}`);
            console.log(`–ü–æ–ª–∏—Ç–∏—á–µ—Å–∫–∏—Ö —Å–æ–±—ã—Ç–∏–π: ${politicalEvents}`);
        }
        
        updateStats();
    </script>
